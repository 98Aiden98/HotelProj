{
  "name": "@hotelproj/webapp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "dotenv -e .env.test -e .env jest",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@hotelproj/backend": "workspace:*",
    "@nanostores/react": "^1.0.0",
    "@sentry/react": "^9.12.0",
    "@tanstack/react-query": "^5.73.3",
    "@trpc/client": "^11.1.0",
    "@trpc/react-query": "^11.1.0",
    "@trpc/server": "^11.1.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.63.0",
    "nanostores": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-image-gallery": "^1.4.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^7.5.0",
    "reset-css": "^5.0.2",
    "sass": "^1.86.3",
    "superjson": "^2.2.2",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^3.3.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.16",
    "@types/mixpanel-browser": "^2.54.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/react-image-gallery": "^1.2.4",
    "@types/react-infinite-scroller": "^1.2.5",
    "@vitejs/plugin-legacy": "^6.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.18.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "vite": "^6.2.6",
    "vite-plugin-svgr": "^4.3.0"
  }
}